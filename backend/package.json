{
  "name": "jni-backend",
  "version": "1.0.0",
  "description": "Jamnasindo API Backend - Express.js + Supabase",
  "main": "api/index.js",
  "scripts": {
    "dev": "node --watch api/index.js",
    "start": "node api/index.js",
    "seed": "node seed.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write \"**/*.js\"",
    "format:check": "prettier --check \"**/*.js\"",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.1",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "winston": "^3.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "vitest": "^1.3.0"
  },
  "engines": {
    "node": ">=18"
  }
}
